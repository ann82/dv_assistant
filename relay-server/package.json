{
  "name": "relay-server",
  "version": "1.0.3",
  "description": "Relay server for the Domestic Violence Support Assistant",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "gpt-tokenizer": "^2.1.2",
    "openai": "^4.103.0",
    "p-throttle": "^6.0.0",
    "raw-body": "^2.5.2",
    "twilio": "^4.22.0",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "0.20.2",
    "supertest": "^7.1.1",
    "typescript": "^5.0.0",
    "vite": "5.2.6",
    "vite-node": "1.3.1",
    "vitest": "1.3.1"
  },
  "files": [
    "lib/**/*",
    "routes/**/*",
    "server.js",
    "websocketServer.js"
  ],
  "exports": {
    ".": "./server.js",
    "./lib/*": "./lib/*.js",
    "./routes/*": "./routes/*.js"
  }
}
